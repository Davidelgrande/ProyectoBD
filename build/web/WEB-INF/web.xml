<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ProductoCTO</servlet-name>
        <servlet-class>controlador.ProductoCTO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioCTO</servlet-name>
        <servlet-class>controlador.UsuarioCTO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MiLogin</servlet-name>
        <servlet-class>controlador.MiLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>AdminCTO</servlet-name>
        <servlet-class>controlador.AdminCTO</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProductoCTO</servlet-name>
        <url-pattern>/ProductoCTO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioCTO</servlet-name>
        <url-pattern>/UsuarioCTO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MiLogin</servlet-name>
        <url-pattern>/MiLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminCTO</servlet-name>
        <url-pattern>/AdminCTO</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.xhtml</welcome-file>
    </welcome-file-list>
</web-app>
